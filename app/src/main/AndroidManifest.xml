<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cisetech.slidingmenu.slidingmenudemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SimpleMenuActivtiy" />
        <activity android:name=".classicalmenu.ClassicalMenuActivity" />
        <activity android:name=".drawerlayout.DrawerLayoutActivity"></activity>
        <activity android:name=".classicalmenu.CustomScaleAnimation"></activity>
        <activity android:name=".classicalmenu.CustomSlideAnimation"></activity>
        <activity android:name=".classicalmenu.CustomZoomAnimation"></activity>
        <activity android:name=".classicalmenu.AttachExample"></activity>
        <activity android:name=".classicalmenu.ResponsiveUIActivity"></activity>
    </application>

</manifest>
